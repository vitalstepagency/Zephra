# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_ANON_KEY=your-anon-key

# Stripe Configuration
STRIPE_SECRET_KEY=
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_...
STRIPE_WEBHOOK_SECRET=
NEXT_PUBLIC_STRIPE_PRICE_ID_BASIC_MONTHLY=price_xxx
NEXT_PUBLIC_STRIPE_PRICE_ID_BASIC_YEARLY=price_xxx
NEXT_PUBLIC_STRIPE_PRICE_ID_PRO_MONTHLY=price_xxx
NEXT_PUBLIC_STRIPE_PRICE_ID_PRO_YEARLY=price_xxx
NEXT_PUBLIC_STRIPE_PRICE_ID_ELITE_MONTHLY=price_xxx
NEXT_PUBLIC_STRIPE_PRICE_ID_ELITE_YEARLY=price_xxx

# Security Configuration
NEXTAUTH_SECRET=
NEXTAUTH_URL=https://your-domain.com
ENCRYPTION_KEY=your-256-bit-encryption-key
JWT_SECRET=your-jwt-secret-key

# Rate Limiting
REDIS_URL=
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-redis-token

# Monitoring & Logging
SENTRY_DSN=https://your-sentry-dsn
